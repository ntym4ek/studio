<?php

/**
 * Implements hook_preprocess_page()
 */
function swiper_preprocess_page(&$vars)
{
  drupal_add_css(drupal_get_path('module', 'swiper') . '/swiper-bundle.min.css');
  drupal_add_js(drupal_get_path('module', 'swiper') . '/swiper-bundle.min.js');
  drupal_add_js(drupal_get_path('module', 'swiper') . '/swiper.js');
}

